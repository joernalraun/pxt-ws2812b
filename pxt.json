{
    "name": "ws2812b",
    "version": "0.1.2",
    "description": "A driver for WS2812B programmable LEDs in MakeCode",
    "license": "MIT",
    "dependencies": {
        "core": "*"
    },
    "files": [
        "README.md",
        "main.ts",
        "sendBuffer.asm",
        "setBufferMode.asm"
    ],
    "testFiles": [
        "test.ts"
    ],
    "public": true,
    "supportedTargets": [
        "microbit",
        "calliopemini"
    ],
    "preferredEditor": "tsprj",
    "yotta": {
        "config": {
            "microbit-dal": {
                "bluetooth": {
                    "enabled": 1,
                    "pairing_mode": 1,
                    "private_addressing": 0,
                    "whitelist": 0,
                    "advertising_timeout": 0,
                    "tx_power": 6,
                    "dfu_service": 1,
                    "event_service": 0,
                    "device_info_service": 1,
                    "security_level": "SECURITY_MODE_ENCRYPTION_OPEN_LINK"
                }
            }
        }
    },
    "binaryonly": true
}
